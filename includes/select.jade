div.selectvars
  select#race
    option(value='0') White
    option(value='1') Hispanic
    option(value='2') Black
    option(value='3') Asian
  select#gender
    option(value='0') Male
    option(value='1') Female
  select#age
    option(value='0') 0-5
    option(value='1') 6-13
    option(value='2') 14-18
    option(value='3') 18-25
    option(value='4') 26-35
    option(value='5') 36-45
    option(value='6') 46-55
    option(value='7') 56-65
    option(value='8') 66-75
    option(value='9') &gt; 75
